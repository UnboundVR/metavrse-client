{
  "name": "metavrse.io-client",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:UnboundVR/metavrse-client.git"
  },
  "dependencies": {
    "bluebird": "^3.3.4",
    "codemirror": "^5.11.0",
    "http-server": "^0.9.0",
    "minecraft-skin": "^0.1.2",
    "pointer-lock": "0.0.4",
    "socket.io-client": "^1.4.5",
    "toolbar": "0.0.5",
    "voxel-engine": "~0.20.2",
    "voxel-fly": "~0.1.0",
    "voxel-highlight": "0.0.10",
    "voxel-player": "0.1.0",
    "voxel-walk": "https://github.com/flyswatter/voxel-walk.git",
    "vue": "^1.0.16",
    "vue-moment": "^1.0.7",
    "script-executor": "https://github.com/UnboundVR/script-executor.git"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "browserify-hmr": "^0.3.1",
    "del": "^2.2.0",
    "eslint": "^2.4.0",
    "eslint-plugin-vue": "^0.1.0",
    "eventemitter2": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-sass": "^2.2.0",
    "gulp-util": "^3.0.7",
    "localenvify": "^1.0.1",
    "lodash.assign": "^4.0.3",
    "npm-run-all": "^1.6.0",
    "strictify": "^0.2.0",
    "three": "0.56.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "vue-hot-reload-api": "^1.3.2",
    "vueify": "^8.3.6",
    "vueify-insert-css": "^1.0.0",
    "watchify": "^3.7.0",
    "winston": "^2.2.0"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify",
      "localenvify"
    ]
  },
  "scripts": {
    "serve": "http-server -c 1 -a localhost -p 1337",
    "watch-js": "watchify -vd -e src -o build/metavrse.js",
    "watch-js-hmr": "watchify -vd -p browserify-hmr -e src -o build/metavrse.js",
    "watch-css": "gulp watch-css",
    "watch": "npm-run-all --parallel watch-js serve watch-css",
    "watch-hmr": "npm-run-all --parallel watch-js-hmr serve watch-css",
    "start": "browserify -e src -o build/metavrse.js && npm run serve"
  }
}
